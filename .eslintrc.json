{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["standard", "plugin:prettier/recommended","eslint:recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console":"warn"
    }
}
